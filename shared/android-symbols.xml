<?xml version="1.0"?>
<keyboard>
    <keyMap modifiers="sym">
        <map iso="D01" to="1" longPress="¹ ½ ⅓ ¼ ⅛"/> <!-- Q -->
        <map iso="D02" to="2" longPress="² ⅔"/> <!-- W -->
        <map iso="D03" to="3" longPress="³ ¾ ⅜"/> <!-- E -->
        <map iso="D04" to="4" longPress="⁴"/> <!-- R -->
        <map iso="D05" to="5" longPress="⅝"/> <!-- T -->
        <map iso="D06" to="6"/> <!-- Y -->
        <map iso="D07" to="7" longPress="⅞"/> <!-- U -->
        <map iso="D08" to="8"/> <!-- I -->
        <map iso="D09" to="9"/> <!-- O -->
        <map iso="D10" to="0" longPress="ⁿ ∅"/> <!-- P -->
        <map iso="C01" to="@"/> <!-- A -->
        <map iso="C02" to="#"/> <!-- S -->
        <map iso="C03" to="$" longPress="¢ £ € ¥ ₱"/> <!-- D -->
        <map iso="C04" to="%" longPress="‰"/> <!-- F -->
        <map iso="C05" to="&amp;"/> <!-- G -->
        <map iso="C06" to="-" longPress="_ – — ·"/> <!-- H -->
        <map iso="C07" to="+" longPress="±"/> <!-- J -->
        <map iso="C08" to="(" longPress="&lt; { ["/> <!-- K -->
        <map iso="C09" to=")" longPress="&gt; } ]"/> <!-- L -->
        <map iso="B01" to="*" longPress="† ‡ ★"/> <!-- Z -->
        <map iso="B02" to="\u{22}" longPress="“ ” « »"/> <!-- X  to= "  -->
        <map iso="B03" to="&apos;" longPress="‘ ’ ‹ ›"/> <!-- C -->
        <map iso="B04" to=":"/> <!-- V -->
        <map iso="B05" to=";"/> <!-- B -->
        <map iso="B06" to="!" longPress="¡"/> <!-- N -->
        <map iso="B07" to="?" longPress="¿"/> <!-- M -->
        <map iso="A00" to="_"/> <!-- (3 keys to left of space) -->
        <map iso="A01" to="/"/> <!-- (2 keys to left of space) -->
        <map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
        <map iso="A03" to=","/> <!-- space -->
        <map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
    </keyMap>
    <keyMap modifiers="sym+shift">
        <map iso="D01" to="~"/> <!-- Q -->
        <map iso="D02" to="`"/> <!-- W -->
        <map iso="D03" to="|"/> <!-- E -->
        <map iso="D04" to="•" longPress="♪ ♥ ♠ ♦ ♣"/> <!-- R -->
        <map iso="D05" to="√"/> <!-- T -->
        <map iso="D06" to="Π" longPress="π"/> <!-- Y -->
        <map iso="D07" to="÷"/> <!-- U -->
        <map iso="D08" to="×"/> <!-- I -->
        <map iso="D09" to="¶" longPress="§"/> <!-- O -->
        <map iso="D10" to="∆"/> <!-- P -->
        <map iso="C01" to="£"/> <!-- A -->
        <map iso="C02" to="¢"/> <!-- S -->
        <map iso="C03" to="€"/> <!-- D -->
        <map iso="C04" to="¥"/> <!-- F -->
        <map iso="C05" to="^" longPress="↑ ↓ ← →"/> <!-- G -->
        <map iso="C06" to="°" longPress="′ ″"/> <!-- H -->
        <map iso="C07" to="=" longPress="≠ ≈ ∞"/> <!-- J -->
        <map iso="C08" to="{"/> <!-- K -->
        <map iso="C09" to="}"/> <!-- L -->
        <map iso="B01" to="\"/> <!-- Z -->
        <map iso="B02" to="©"/> <!-- X -->
        <map iso="B03" to="®"/> <!-- C -->
        <map iso="B04" to="™"/> <!-- V -->
        <map iso="B05" to="℅"/> <!-- B -->
        <map iso="B06" to="["/> <!-- N -->
        <map iso="B07" to="]"/> <!-- M -->
        <map iso="A00" to="&lt;" longPress="‹ ≤ «"/> <!-- (3 keys to left of space) -->
        <map iso="A01" to="&gt;" longPress="› ≥ »"/> <!-- (2 keys to left of space) -->
        <map iso="A02" to=" "/> <!-- (key to left of space)  base=, -->
        <map iso="A03" to=","/> <!-- space -->
        <map iso="A04" to="." longPress="…"/> <!-- (key to right of space) -->
    </keyMap>

    <layer modifier="sym">
        <row keys="D01-D10"/>
        <row keys="C01-C09"/>
        <row keys="shift B01-B07 bksp"/>
        <row keys="base A03 numpad A02 A04 enter"/>
        <switch iso="shift" layer="sym+shift" keycap="=\<"/>
        <switch iso="base" layer="none" keycap="ABC"/>
        <vkeys type="windows">
            <vkey iso="D01" vkey="1"/>
            <vkey iso="D02" vkey="2"/>
            <vkey iso="D03" vkey="3"/>
            <vkey iso="D04" vkey="4"/>
            <vkey iso="D05" vkey="5"/>
            <vkey iso="D06" vkey="6"/>
            <vkey iso="D07" vkey="7"/>
            <vkey iso="D08" vkey="8"/>
            <vkey iso="D09" vkey="9"/>
            <vkey iso="D10" vkey="0"/>
            <vkey iso="C01" vkey="2" modifier="shift"/>
            <vkey iso="C02" vkey="3" modifier="shift"/>
            <vkey iso="C03" vkey="4" modifier="shift"/>
            <vkey iso="C04" vkey="5" modifier="shift"/>
            <vkey iso="C05" vkey="7" modifier="shift"/>
            <vkey iso="C06" vkey="HYPHEN"/>
            <vkey iso="C07" vkey="EQUALS" modifier="shift"/>
            <vkey iso="C08" vkey="9" modifier="shift"/>
            <vkey iso="C09" vkey="0" modifier="shift"/>
            <vkey iso="B01" vkey="8" modifier="shift"/>
            <vkey iso="B02" vkey="QUOTE" modifier="shift"/>
            <vkey iso="B03" vkey="QUOTE"/>
            <vkey iso="B04" vkey="COLON" modifier="shift"/>
            <vkey iso="B05" vkey="COLON"/>
            <vkey iso="B06" vkey="1" modifier="shift"/>
            <vkey iso="B07" vkey="SLASH" modifier="shift"/>
            <vkey iso="A00" vkey="HYPHEN" modifier="shift"/>
            <vkey iso="A01" vkey="SLASH"/>
            <vkey iso="A02" vkey="SPACE"/>
            <vkey iso="A03" vkey="COMMA"/>
            <vkey iso="A04" vkey="PERIOD"/>
        </vkeys>
    </layer>
    <layer modifier="sym+shift">
        <row keys="D01-D10"/>
        <row keys="C01-C09"/>
        <row keys="sym B01-B07 bksp"/>
        <row keys="base A03 numpad A02 A04 enter"/>
        <switch iso="sym" layer="sym" keycap="?123"/>
        <switch iso="base" layer="none" keycap="ABC"/>
        <vkeys type="windows">
            <vkey iso="D01" vkey="BQUOTE" modifier="shift"/>
            <vkey iso="D02" vkey="BQUOTE"/>
            <vkey iso="D03" vkey="BSLASH" modifier="shift"/>
            <vkey iso="C05" vkey="6" modifier="shift"/>
            <vkey iso="C08" vkey="LBRACKET" modifier="shift"/>
            <vkey iso="C09" vkey="RBRACKET" modifier="shift"/>
            <vkey iso="B01" vkey="BSLASH"/>
            <vkey iso="B06" vkey="LBRACKET"/>
            <vkey iso="B07" vkey="RBRACKET"/>
            <vkey iso="A00" vkey="COMMA" modifier="shift"/>
            <vkey iso="A01" vkey="PERIOD" modifier="shift"/>
            <vkey iso="A02" vkey="SPACE"/>
            <vkey iso="A03" vkey="COMMA"/>
            <vkey iso="A04" vkey="PERIOD"/>
        </vkeys>
    </layer>
</keyboard>
