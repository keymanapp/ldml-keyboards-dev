<?xml version="1.0"?>
<keyboard>
    <import path="android-symbols.xml"/>
    <layer modifier="none">
        <row keys="D01-D10"/>
        <row keys="C01-C09"/>
        <row keys="shift B01-B07 bksp"/>
        <row keys="sym A01 smilies A02-A03 enter"/>
        <switch iso="sym" layer="sym" keycap="?123"/>
        <switch iso="shift" layer="shift" keycap="&#x21EA;"/>
        <!-- vkeys fall through to default from physical keyboard -->
    </layer>
    <layer modifier="shift">
        <row keys="D01-D10"/>
        <row keys="C01-C09"/>
        <row keys="shift B01-B07 bksp"/>
        <row keys="sym A01 smilies A02-A03 enter"/>
        <switch iso="sym" layer="sym" keycap="?123"/>
        <switch iso="shift" layer="none" keycap="&#x21EA;" style="highlight"/>
        <!-- vkeys from shift default due to magic shift name -->
    </layer>
</keyboard>
