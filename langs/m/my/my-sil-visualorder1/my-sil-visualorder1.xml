    <reorders>
        <!-- only add information or override not replace -->
        <reorder from="\u1031" prebase="1"/>
        <reorder from="\u103C" prebase="1"/>
        <reorder from="[\u1004\u101B\u105A]\u103A\u1039" order="-1"/>
    </reorders>

